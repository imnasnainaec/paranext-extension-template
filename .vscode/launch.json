    "version": "0.2.0",
    "configurations": [
      {
        "name": "Watch and Build Extension",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "npm",
        "skipFiles": ["<node_internals>/**"],
        "runtimeArgs": ["run", "start"]
      },
      {
        "name": "Build Extension",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "npm",
        "skipFiles": ["<node_internals>/**"],
        "runtimeArgs": ["run", "build"]
      }
    ]
}